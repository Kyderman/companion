					<li class="next"><a href="#">Next</a></li>
				</ul>
				<div class="list-group">
					<%@teams.each do |t|%>
					<a href="#" class="list-group-item"> 
						<%=t.name%>
						<span class="hidden-xs pull-right">
							<span class="badge"><%=t.sport%></span>
						</span>
						<span class="visible-xs">
							<div class="row container">
							<span class="badge"><%=t.sport%></span>
							</div>
						</span>
					</a>
